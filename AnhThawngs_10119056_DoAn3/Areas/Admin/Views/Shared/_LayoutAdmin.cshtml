                            <li>
                                <a href="/Admin/AdminLSP/Index">Quản lý loại linh kiện</a>
                            </li>
                            <li>
                                <a href="/Admin/AdminSP/Index">Quản lý linh kiện</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript:;">
                            <i class="sidebar-item-icon fa fa-calendar"></i>
                            <span class="nav-label">Hóa đơn</span><i class="fa fa-angle-left arrow"></i>
                        </a>
                        <ul class="nav-2-level collapse">
                            <li>
                                <a href="#">Thông tin hóa đơn</a>
                            </li>
